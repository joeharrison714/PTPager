@model IEnumerable<PTPager.Web2.Models.SpeechHistoryItem>
@{
    Layout = null;

}



<html>

<table>
    <tr>
        <th>Date</th>
        <th>Announcement</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Date</td>
            <td>@item.Speech</td>
        </tr>
    }
</table>

